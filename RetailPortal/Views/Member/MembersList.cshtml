@model List<Members>
@{
    ViewData["Title"] = "Members";
}

<!DOCTYPE html>
<html>
<head>
    <title>@ViewData["Title"]</title>
    <style>
        table {
            width: 100%;
            border-collapse: collapse;
        }

        th, td {
            padding: 8px;
            text-align: left;
            border-bottom: 1px solid #ddd;
        }

        th {
            background-color: #f2f2f2;
        }

        tbody tr:hover {
            background-color: #f5f5f5;
        }
    </style>
</head>
<body>
    <h1>@ViewData["Title"]</h1>
    <table>
        <thead>
            <tr>
                <th>Row Number</th>
                <th>Quotation Member ID</th>
                <th>Quotation Id</th>
                <th>Member Type</th>
        </thead>
        <tbody>
            @if (Model != null && Model.Any())
            {
                foreach (var item in Model.Take(5))
                {
                    <tr>
                        <td>@item.RowNumber</td>
                        <td>@item.GmquotationMemberId</td>
                        <td>@item.GmquotationId</td>
                        <td>@item.MemberType</td>
                    </tr>
                }
            }
            else
            {
                <tr>
                    <td colspan="5">No Members found.</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
